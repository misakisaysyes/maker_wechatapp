.title{
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.container{
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.header{
  display:flex;
  flex-direction: column;
  width:100%;
  align-items: center;
  padding-top: 50px;
  padding-bottom: 65px;
}

.body{
  display:flex;
  flex-direction: column;
  width:100%;
  align-items: center;
}

.footer{
  display:flex;
  flex-direction: row;
  margin-top:25px;
  margin-bottom:80px;
}

.picker{
  width: 75%;
  margin-bottom:23px;
  display: flex;
  flex-direction: row;
  border: 1px solid #999999; 
}

.inputbox{
  border-bottom:1px solid #999999;
  margin-bottom:23px;
  width:75%;
}

.tabox{
  height:5em;
  width:75%; 
  border:1px solid #999999;
  margin-bottom:23px;
}

.tag1{
  border-bottom:1px solid #999999;
  margin-left:15px;
  margin-right:27px
}

.tag2{
  border-bottom:1px solid #999999;
  margin-right:27px;
}

.tag3{
  border-bottom:1px solid #999999; 
}

.box{
  margin-left:0.5em;
  width:16.8em;
  display: flex;
  justify-content: space-between;
}